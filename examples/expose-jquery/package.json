{
  "name": "shim-jquery",
  "version": "0.0.0",
  "description": "Example of using browserify-shim to expose jquery for use with browserify",
  "main": "./js/entry.js",
  "browserify-shim": {
    "jquery": "global:$"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "repository": "",
  "author": "Thorsten Lorenz",
  "license": "BSD",
  "dependencies": {
    "request": "~2.12.0"
  },
  "devDependencies": {
    "browserify": "~2.36.1",
    "browserify-shim": "~3.2.0"
  }
}
