{
  "name": "shim-jquery",
  "version": "0.0.0",
  "description": "Example of using browserify-shim to shim jquery for use with browserify",
  "main": "./js/entry.js",
  "browser": {
    "jquery": "./js/vendor/jquery.js"
  },
  "browserify-shim": {
    "jquery": "$"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": "",
  "author": "Thorsten Lorenz",
  "license": "BSD",
  "dependencies": {
    "request": "~2.88.0"
  },
  "devDependencies": {
    "browserify": "~2.36.1",
    "browserify-shim": "~3.0.0"
  }
}
